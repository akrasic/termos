#--------------------------
# So cool aliases
#--------------------------
alias ll='ls -lah'
alias s="ssh -l root"
alias bim="vim"

#--------------------------
#   Git aliases
#--------------------------
alias g="git"
alias ga="git add"
alias gs="git status"
alias gc="git commit"
alias gcs="git commit -S"
alias gb="git branch"
alias gco="git checkout"
